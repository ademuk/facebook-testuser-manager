RewriteEngine On

php_value xdebug.profiler_enable 1  

RewriteCond %{REQUEST_FILENAME} -s [OR]
RewriteCond %{REQUEST_FILENAME} -l [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^.*$ - [NC,L]
RewriteRule ^.*$ index.php [NC,L]

AuthType Basic
AuthName "Facebook Test User Manager"
#AuthUserFile /var/www/facebook-testuser-manager/public/.htpasswd
AuthUserFile /Users/adem/Documents/www/facebook-testuser-manager/public/.htpasswd
Require valid-user